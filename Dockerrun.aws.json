{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "fibonacci-client",
			"image": "varxmeh/fibonacci-client",
			"hostname": "fb_client",
			"essential": false
		},
		{
			"name": "fibonacci-server",
			"image": "varxmeh/fibonacci-server",
			"hostname": "fb_api",
			"essential": false
		},
		{
			"name": "fibonacci-worker",
			"image": "varxmeh/fibonacci-worker",
			"hostname": "fb_worker",
			"essential": false
		},
		{
			"name": "fibonacci-nginx",
			"image": "varxmeh/fibonacci-nginx",
			"hostname": "fb_nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["fibonacci-client", "fibonacci-server"]
		}
	]
}
